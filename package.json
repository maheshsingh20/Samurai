{
  "name": "activity-feed-system",
  "version": "1.0.0",
  "description": "Mini activity feed + notifications system like GitHub/LinkedIn",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon backend/server.js",
    "client": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "node backend/server.js",
    "load-test": "node scripts/load-test.js",
    "setup": "npm install && cd frontend && npm install",
    "seed": "node scripts/seed-data.js seed",
    "seed-continuous": "node scripts/seed-data.js continuous",
    "seed-burst": "node scripts/seed-data.js burst"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "mongoose": "^8.0.3",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "autocannon": "^7.12.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "activity-feed",
    "notifications",
    "real-time",
    "mern",
    "vite"
  ],
  "author": "",
  "license": "MIT"
}
